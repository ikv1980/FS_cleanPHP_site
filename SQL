CREATE TABLE `users` (`id` INT(11) UNSIGNED NOT NULL AUTO_INCREMENT , `name` VARCHAR(70) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL , `email` VARCHAR(100) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL , `pass` VARCHAR(255) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL , PRIMARY KEY (`id`)) ENGINE = MyISAM CHARSET=utf8 COLLATE utf8_general_ci;
